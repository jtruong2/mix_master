<%= form_for @artist do |f| %>
  <% if @artist.errors.any? %>
    <h2><%= pluralize(@artist.errors.count, "error") %> prohibited this record form being saved:</h2>
    <ul>
      <% @artist.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
    </ul>
  <% end %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label "image url" %>
  <%= f.text_area :image %>

  <%= f.submit "Save Artist!" %>
<% end %>
